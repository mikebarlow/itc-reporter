{
    "name": "snscripts/itc-reporter",
    "description": "PHP package for downloading stats from iTunes Connect - Replacement for Apples Java based Reporter.jar",
    "keywords": ["iTunes", "Connect", "Reporter.jar", "App Stats"],
    "homepage": "https://github.com/snscripts/itc-reporter",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Barlow",
            "email": "mike@mikebarlow.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Snscripts\\ITCReporter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snscripts\\ITCReporter\\Tests\\": "tests"
        }
    }
}